<h1>Add Contact to <%= @organization.name %></h1>
  <%= form_for [@organization, @contact] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br><br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br><br>
  <%= f.label :email %>
  <%= f.email_field :email %>
  <br><br>
  <%= f.label :notes%>
  <%= f.text_area :notes %>
  <br><br>
  <%= hidden_field_tag "contact[organization_id]", @organization.id %>
  <%= f.submit %>
<% end %>
