<h1>List of Events</h1>
<h4><%= link_to "New Event", new_event_path %></h4>
<hr>
<ul>
  <% @events.each do |event| %>
    <li <%= li_id_past_due(event) %> ><%= presentable_date(event.date) %> - <%= event.organization.name %> - <%= link_to event.name, event_path(event) %></li>
  <% end %>
</ul>

<% @events_by_timeframe.each do |event| %>
  <%= event.name %><br>
<% end %>
